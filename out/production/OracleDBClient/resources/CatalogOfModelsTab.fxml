      </VBox>
      <VBox layoutX="168.0" layoutY="30.0" prefHeight="283.0" prefWidth="222.0" spacing="10.5">
         <children>
            <TextField fx:id="idKMTextField" disable="true" />
            <TextField fx:id="modelTextField" />
            <TextField fx:id="brandTextField" />
            <TextField fx:id="categoryTextField" />
            <TextField fx:id="dmcTextField" />
            <TextField fx:id="seatsTextField" />
            <TextField fx:id="specificInfoTextField" />
            <TextField fx:id="priceTextField" />
